<h1 class="">Schwartz Lab Maintenance Page</h1>
<h3><%= link_to "Home", root_path%></h3>

<h2>Add a profile </h2>

<div class="rows">
  <div class="columns small-6" style="background-color:#FFF; border:dashed 2px black;">
    <%= form_for @profile do |f| %>

    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :email %>
    <%= f.text_field :email %>

    <%= f.label :description %>
    <%= f.text_field :description %>

    <%= f.label :profile_image %>
    <%= f.file_field :profile_image %>
    
    <%= f.submit "Add New Profile" %>
    <% end %>
  </div>
</div>
